{
	"name": "jolt-extension",
	"displayName": "jolt-extension",
	"description": "Jolt extension for VS Code",
	"version": "1.2.0",
	"publisher": "br93",
	"repository": "https://github.com/br93/jolt-extension-vs-code",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.jolt",
				"title": "Jolt (Transform)"
			},
			{
				"command": "openWindow.jolt",
				"title": "JOLT (Create input/spec)"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "webview",
					"title": "Jolt extension for VSCode",
					"icon": "./media/jolt.svg"
				}
			]
		},
		"views": {
			"webview": [
				{
					"type": "webview",
					"id": "jolt-webview",
					"name": "Jolt extension for VSCode",
					"icon": "src/media/jolt.svg"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^16.18.34",
		"@types/node-fetch": "^2.6.5",
		"@types/react": "^18.2.27",
		"@types/react-dom": "^18.2.12",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"eslint": "^8.26.0",
		"react-dom": "^18.2.0",
		"typescript": "^5.2.2"
	},
	"dependencies": {
		"module-alias": "^2.2.3",
		"node-fetch": "^2.7.0",
		"react": "^18.2.0",
		"text-encoding": "^0.7.0"
	}
}
